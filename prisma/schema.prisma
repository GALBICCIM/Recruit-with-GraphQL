generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @default(autoincrement()) @id
  name String
  student_id String
  phone_number String
  email String
  password String
}

model Form {
  id Int @default(autoincrement()) @id
  applicant User @relation(fields: [applicantId], references: [id])
  applicantId Int
  intro_self String
	because String
	capability String
	ambition String
}
